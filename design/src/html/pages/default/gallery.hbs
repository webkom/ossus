                <li class="item fileinput fileinput-new" data-provides="fileinput">
                    <div class="fileinput-preview fileinput-exists"></div>
                    <div class="details">
                        <div class="action btn-file">
                            <span class="fileinput-new action-file">
                                <i class="fa fa-cloud-upload"></i>
                            </span>
                            <p class="fileinput-new">Click here or drag and drop to add images</p>
                            <i class="fa fa-pencil-square-o fileinput-exists"></i>
                            <input type="file" name="file"/>
                        </div>
                        <a href="#" class="action fileinput-exists" data-dismiss="fileinput"><i class="fa fa-trash-o"></i></a>
                    </div>
                </li>
                <li class="item">
                    <img alt="150x150" src="img/gallery/1H.jpg" />
